{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": [
      "ES2022"
    ],
    "allowJs": true,
    "checkJs": true,
		"resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": "src",
		"paths": {
			"@ags/*": ["../types/*"]
		},
    "typeRoots": [
      "./types"
    ],
    "skipLibCheck": true,
		"moduleResolution": "node"
  }
}
